{
    "$id": "https://vng.brp.nl/schemas/brp.overlijdenInOnderzoek.schema.json",
    "$schema": "https://docs.commongateway.nl/schemas/Entity.schema.json",
    "version": "0.1.0",
    "title": "OverlijdenInOnderzoek",
    "description": "Geeft aan welke gegevens over het overlijden van de persoon in onderzoek zijn. Zie de [functionele specificaties](https://github.com/VNG-Realisatie/Haal-Centraal-BRP-bevragen/blob/v1.1.0/features/in_onderzoek.feature)",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "format": "uuid",
            "nullable": true
        },
        "datum": {
            "type": "boolean"
        },
        "land": {
            "type": "boolean"
        },
        "plaats": {
            "type": "boolean"
        },
        "datumIngangOnderzoek": {
            "$ref": "https://vng.brp.nl/schemas/brp.datumOnvolledig.schema.json"
        }
    }
}