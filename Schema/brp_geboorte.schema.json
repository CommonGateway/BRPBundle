{
    "$id": "https://vng.opencatalogi.nl/schemas/brp_geboorte.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "version": "0.1.0",
    "Geboorte": {
        "allOf": [
            {
                "$ref": "#/components/schemas/Geboortedatum"
            },
            {
                "properties": {
                    "land": {
                        "$ref": "#/components/schemas/Waardetabel"
                    },
                    "plaats": {
                        "$ref": "#/components/schemas/Waardetabel"
                    },
                    "inOnderzoek": {
                        "$ref": "#/components/schemas/GeboorteInOnderzoek"
                    }
                },
                "description": "Gegevens over de geboorte.\n* **datum** : datum waarop de persoon is geboren.\n* **land** : land waar de persoon is geboren\n* **plaats** : gemeente waar de persoon is geboren. Is de persoon geboren buiten Nederland, dan bevat het antwoord alleen een buitenlandse plaatsnaam of aanduiding.\n"
            }
        ]
    }
}